{
    "dependencies": {
        "python": {
            "version": "latest"
        }
    },
    "installation": {
        "name": "Creating environment",
        "env": "env",
        "commands": [
            "pip install --upgrade pip",
            "pip install open-webui"
        ]
    },
    "start": {
        "name": "Starting open-webui",
        "env": "env",
        "commands": [
            "open-webui serve"
        ]
    }
}